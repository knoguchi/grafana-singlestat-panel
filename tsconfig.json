{
  "compilerOptions": {
    "module": "system",
    "target": "es5",
    "rootDir": "./src",
    "declaration": false,
    "allowSyntheticDefaultImports": true,
    "inlineSourceMap": false,
    "sourceMap": true,
    "noEmitOnError": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "noImplicitUseStrict":false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "types" : ["node"]

  }
}
